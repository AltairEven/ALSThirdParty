{
  "name": "ALSThirdParty",
  "version": "0.0.1",
  "summary": "This is a ALSThirdParty.",
  "description": "This is a long description of ALSThirdParty",
  "homepage": "https://github.com/AltairEven/ALSThirdParty.git",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "AltairEven": "qianye.qy@alibaba-inc.com"
  },
  "source": {
    "git": "https://github.com/AltairEven/ALSThirdParty.git",
    "tag": "0.0.1"
  },
  "platforms": {
    "ios": "8.0"
  },
  "default_subspecs": "Core",
  "subspecs": [
    {
      "name": "Core",
      "source_files": "ALSThirdParty/Classes/**/*",
      "public_header_files": "ALSThirdParty/Classes/**/*.h",
      "resources": "ALSThirdParty/Classes/*.bundle",
      "frameworks": [
        "CoreTelephony",
        "SystemConfiguration"
      ],
      "ios": {
        "libraries": [
          "stdc++.6.0.9",
          "c++",
          "z.1.2.8",
          "sqlite3.0"
        ]
      },
      "xcconfig": {
        "OTHER_LDFLAGS": "-ObjC",
        "ENABLE_BITCODE": "NO"
      },
      "vendored_frameworks": "ALSThirdParty/Classes/**/*.framework",
      "vendored_libraries": "ALSThirdParty/Classes/**/*.a"
    }
  ]
}
